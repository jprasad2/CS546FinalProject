{{title}} <br>

{{#if User}}
    <p class="profile">
        {{User.profilePicture}} <br>
        Name: {{User.firstName}} {{User.lastName}} <br>
        Username: {{User.Username}} <br>
        Email: {{User.Email}} <br>
        Bio: {{User.Bio}} <br>
        Age: {{User.Age}}
    </p>
{{/if}}

{{#each Portfolio}}
    <p class="portfolio">
        Subject: {{Subject}} <br>
        Create Date: {{createDate}} <br>
        Last Update: {{lastUpdate}}
    </p>
{{/each}}